#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int hourglassSum(int arr[6][6]) {
    int maxSum = -63;  
    for (int i = 0; i < 4; i++) {
        for (int j = 0; j < 4; j++) {
            // Calculate the sum of the current hourglass
            int top = arr[i][j] + arr[i][j+1] + arr[i][j+2];
            int middle = arr[i+1][j+1];
            int bottom = arr[i+2][j] + arr[i+2][j+1] + arr[i+2][j+2];
            int hourglass = top + middle + bottom;

            
            if (hourglass > maxSum) {
                maxSum = hourglass;
            }
        }
    }
    return maxSum;
}

int main() {
    int arr[6][6];
    
  
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            scanf("%d", &arr[i][j]);
        }
    }
    
    int result = hourglassSum(arr);
    printf("%d\n", result);
    
    return 0;
}
